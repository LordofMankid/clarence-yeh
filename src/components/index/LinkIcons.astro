---
import { Icon } from "astro-icon/components";

interface Props {}

const {} = Astro.props;
---

<!-- Automatically fetches and inlines Material Design Icon's "account" SVG -->
<div class="container">
  <Icon name="mdi:github" size={35} color="var(--grey)" />
  <Icon name="mdi:linkedin" size={35} color="var(--grey)" />
  <Icon name="ic:outline-email" size={35} color="var(--grey)" />
</div>

<style>
  .container {
    display: flex;
    flex-direction: row;
    height: 24;
  }
</style>
